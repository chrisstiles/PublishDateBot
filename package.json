{
  "name": "publish-date-bot",
  "version": "1.1.0",
  "description": "Looks for outdated articles on Reddit",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "scss": "node-sass --watch public/css -o public/css",
    "server": "nodemon index.js",
    "reddit": "reddit.js",
    "preinstall": "npx npm-force-resolutions"
  },
  "nodemonConfig": {
    "ignore": [
      "reddit.js"
    ]
  },
  "author": "Chris Stiles",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "jsdom": "^16.2.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "pg": "^7.18.2",
    "snoowrap": "^1.21.0",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "minimist": "^1.2.5",
    "nodemon": "^1.19.4"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  }
}
