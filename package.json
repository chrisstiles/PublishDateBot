{
  "name": "publish-date-bot",
  "version": "1.5.2",
  "description": "Looks for outdated articles on Reddit",
  "engines": {
    "node": "16.13.x",
    "npm": "8.3.x"
  },
  "type": "module",
  "main": "./src/server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "scss": "node-sass --watch ./src/public/css -o ./src/public/css --output-style compressed",
    "server": "nodemon ./src/server.js",
    "reddit": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 --unhandled-rejections=warn ./src/reddit.js",
    "preinstall": "npx npm-force-resolutions"
  },
  "nodemonConfig": {
    "ignore": [
      "reddit.js"
    ]
  },
  "author": "Chris Stiles",
  "license": "ISC",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "html-entities": "^2.3.3",
    "install": "^0.13.0",
    "jsdom": "^16.2.1",
    "lodash": "^4.17.19",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "node-fetch": "^3.2.0",
    "node-sass": "^7.0.1",
    "npm": "^8.5.2",
    "pg": "^8.6.0",
    "prettier": "^2.7.1",
    "puppeteer": "^19.0.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-adblocker": "^2.13.5",
    "puppeteer-extra-plugin-block-resources": "^2.4.2",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "serve-favicon": "^2.5.0",
    "snoowrap": "^1.23.0",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.7"
  }
}
