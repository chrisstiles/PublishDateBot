{
  "name": "publish-date-bot",
  "version": "1.4.1",
  "description": "Looks for outdated articles on Reddit",
  "engines": {
    "node": "16.2.x",
    "npm": "7.13.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "scss": "node-sass --watch public/css -o public/css",
    "server": "nodemon index.js",
    "reddit": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 --unhandled-rejections=warn reddit.js",
    "preinstall": "npx npm-force-resolutions"
  },
  "nodemonConfig": {
    "ignore": [
      "reddit.js"
    ]
  },
  "author": "Chris Stiles",
  "license": "ISC",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-parser-js": "^0.5.3",
    "jsdom": "^16.2.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^6.0.0",
    "pg": "^8.6.0",
    "snoowrap": "^1.23.0",
    "strip-indent": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.7"
  }
}
